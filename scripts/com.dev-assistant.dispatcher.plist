<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dev-assistant.dispatcher</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Users/kimjin-wan/Works/personal/dev-assistant/scripts/dispatcher.sh</string>
    </array>

    <!-- 5분(300초)마다 실행 -->
    <key>StartInterval</key>
    <integer>300</integer>

    <key>StandardOutPath</key>
    <string>/Users/kimjin-wan/Works/personal/dev-assistant/logs/dispatcher.stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/kimjin-wan/Works/personal/dev-assistant/logs/dispatcher.stderr.log</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/opt/homebrew/bin</string>
        <key>HOME</key>
        <string>/Users/kimjin-wan</string>
    </dict>

    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
